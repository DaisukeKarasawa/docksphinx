<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>io.docksphinx.docksphinxd</string>

  <key>ProgramArguments</key>
  <array>
    <string>/usr/local/bin/docksphinxd</string>
    <string>start</string>
    <string>--config</string>
    <string>/Users/USERNAME/.config/docksphinx/config.yaml</string>
  </array>

  <key>RunAtLoad</key>
  <true/>

  <key>KeepAlive</key>
  <true/>

  <key>StandardOutPath</key>
  <string>/Users/USERNAME/Library/Logs/docksphinx/docksphinxd.log</string>

  <key>StandardErrorPath</key>
  <string>/Users/USERNAME/Library/Logs/docksphinx/docksphinxd.err.log</string>

  <key>WorkingDirectory</key>
  <string>/Users/USERNAME</string>
</dict>
</plist>
